{
  "name": "Website Analyzer",
  "description": "Accessibility analyzer for web pages",
  "version": "2.0",
  "permissions": [
    "activeTab", "http://*/"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Analyze this web page"
  },
  "web_accessible_resources": ["images/x.png"], 
  "manifest_version": 2
}
